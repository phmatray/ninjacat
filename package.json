{
  "name": "ninjacat",
  "description": "a netcore rest api generator written with typescript",
  "version": "0.1.0-alpha1",
  "author": "phmatray @phmatray",
  "license": "MIT",
  "homepage": "https://phmatray.github.io/ninjacat",
  "bugs": "https://github.com/phmatray/ninjacat/issues",
  "repository": "phmatray/ninjacat",
  "keywords": [
    "oclif"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "bin": {
    "ninjacat": "./bin/run"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ninjacat",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "check:dependencies": "npx npm-check -u"
  },
  "dependencies": {
    "@oclif/command": "1.5.19",
    "@oclif/config": "1.13.3",
    "@oclif/plugin-autocomplete": "0.1.4",
    "@oclif/plugin-help": "2.2.1",
    "@oclif/plugin-update": "1.3.9",
    "@oclif/plugin-warn-if-update-available": "1.7.0",
    "chalk": "2.4.2",
    "colors": "1.4.0",
    "cross-spawn": "7.0.0",
    "ejs": "2.7.1",
    "execa": "2.0.4",
    "expect": "24.9.0",
    "figlet": "1.2.4",
    "fs-jetpack": "2.2.2",
    "glob": "7.1.4",
    "inquirer": "7.0.0",
    "lodash": "4.17.15",
    "ora": "4.0.1",
    "pluralize": "8.0.0",
    "semver": "6.3.0",
    "tslib": "1.10.0",
    "which": "1.3.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.2",
    "@oclif/test": "1.2.5",
    "@types/chai": "4.2.3",
    "@types/cross-spawn": "6.0.0",
    "@types/ejs": "2.6.3",
    "@types/figlet": "1.2.0",
    "@types/inquirer": "6.5.0",
    "@types/lowdb": "1.0.9",
    "@types/mocha": "5.2.7",
    "@types/node": "10.14.18",
    "@types/pluralize": "0.0.29",
    "@types/which": "1.3.1",
    "@typescript-eslint/eslint-plugin": "2.3.0",
    "@typescript-eslint/parser": "2.3.0",
    "chai": "4.2.0",
    "eslint": "6.4.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "globby": "10.0.1",
    "inversify": "5.0.1",
    "lowdb": "1.0.0",
    "mocha": "5.2.0",
    "nyc": "13.3.0",
    "prettier": "1.18.2",
    "reflect-metadata": "0.1.13",
    "ts-node": "8.4.1",
    "typescript": "3.6.3"
  }
}
